{
    "elements": [
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "Label",
                    "label": "\nShutter Actuator\n ",
                    "link": false,
                    "bold": true
                }
            ]
        },
        {
            "type": "Label",
            "label": "The shutter control is used to control the correct opening position depending on the motor running time."
        },
        {
            "type": "ExpansionPanel",
            "caption": "Devices ...",
            "items": [
                {
                    "type": "Label",
                    "caption": "Please select the vatiable for receive and transmit the level(position)."
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "SelectVariable",
                            "name": "ReceiverVariable",
                            "caption": "Receiver:",
                            "width": "450px"
                        },
                        {
                            "type": "SelectVariable",
                            "name": "TransmitterVariable",
                            "caption": "Transmitter:",
                            "width": "450px"
                        }
                    ]
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Zeiten ...",
            "items": [
                {
                    "type": "Label",
                    "caption": "Please measure the running time for the respective position and enter the corresponding values or start the automatic calibration!"
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "NumberSpinner",
                            "name": "time_down_50",
                            "caption": "open → middle:",
                            "digits": 2,
                            "suffix": "sec",
                            "width": "300px"
                        },
                        {
                            "type": "NumberSpinner",
                            "name": "time_down_85",
                            "caption": "open → down:",
                            "digits": 2,
                            "suffix": "sec",
                            "width": "300px"
                        },
                        {
                            "type": "NumberSpinner",
                            "name": "time_down_100",
                            "caption": "open → close:",
                            "digits": 2,
                            "suffix": "sec",
                            "width": "300px"
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                      {
                          "type": "NumberSpinner",
                          "name": "time_up_85",
                          "caption": "close → down:",
                          "digits": 2,
                          "suffix": "sec",
                          "width": "300px"
                      },
                      {
                          "type": "NumberSpinner",
                          "name": "time_up_50",
                          "caption": "close → middle:",
                          "digits": 2,
                          "suffix": "sec",
                          "width": "300px"
                      },
                      {
                          "type": "NumberSpinner",
                          "name": "time_up_0",
                          "caption": "close → open:",
                          "digits": 2,
                          "suffix": "sec",
                          "width": "300px"
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                      {
                        "type": "NumberSpinner",
                        "name": "time_start_delay",
                        "caption": "delay on start (sec)",
                        "digits": 1,
                        "suffix": "sec",
                        "width": "300px"
                      },
                      {
                        "type": "NumberSpinner",
                        "name": "time_full_move_extra",
                        "caption": "additional time for full close / open (sec)",
                        "digits": 1,
                        "suffix": "sec",
                        "width": "300px"
                      }
                    ]
                }
            ]
        },
        {
          "type": "ExpansionPanel",
          "caption": "calibration",
          "items": [
            {
              "type": "NumberSpinner",
              "name": "calibration_duration",
              "caption": "calibration time (sec)",
              "digits": 1,
              "suffix": "s",
              "default": 6,
              "save": true
            }
          ]
        },

            {
              "type": "CheckBox",
              "name": "auto_save_calibration",
              "caption": "Kalibrierwerte automatisch speichern"
            }

    ],
    "actions": [
        {
            "type": "Label",
            "caption": "Use the button to measure the right values! The 'SHOW'-Button will display the internal level of the shutter!"
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "Button",
                    "caption": "UP",
                    "onClick": "TSA_Up($id);"
                },
                {
                    "type": "Button",
                    "caption": "STOP",
                    "onClick": "TSA_Stop($id);"
                },
                {
                    "type": "Button",
                    "caption": "DOWN",
                    "onClick": "TSA_Down($id);"
                },
                {
                    "type": "Button",
                    "caption": "SHOW",
                    "onClick": "echo 'Position: '.TSA_Level($id).' %';"
                }
              ]
          },
        {
              "type": "RowLayout",
              "items": [
                {
                  "type": "Button",
                  "caption": "calibration - lower shutters",
                  "onClick": "TSA_CalibrateDown($id);"
                },
                {
                  "type": "Button",
                  "caption": "calibration - raise shutters",
                  "onClick": "TSA_CalibrateUp($id);"
                },
                {
                  "type": "Button",
                  "caption": "calibration - delay",
                  "onClick": "TSA_Calibratedelay($id);"
                }
            ]
        },
        {
            "type": "Label",
            "caption": "\n"
        },
        {
            "type": "ExpansionPanel",
            "caption": "Source code and licence ...",
            "items": [
                {
                    "type": "Label",
                    "caption": "The software is free of charge for non-commercial use.",
                    "bold": true
                },
                {
                    "type": "RowLayout",
                    "items": [
                      {
                          "type": "Image",
                          "image": "data:image/svg+xml;base64,",
                          "onClick": "echo 'https://github.com/madrosDK/ShutterActuator_xcomfort';"
                      },
                        {
                            "type": "Image",
                            "image": "data:image/svg+xml;base64,",
                            "onClick": "echo 'https://creativecommons.org/licenses/by-nc-sa/4.0/';"
                        }
                    ]
                }
            ]
        }
    ]
}
